<?xml version="1.0" encoding="utf-8"?><!--
   Data extraction rules - Exclude SharedPreferences to ensure fresh start on reinstall
   See https://developer.android.com/about/versions/12/backup-restore#xml-changes
-->
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude SharedPreferences to clear all keys on uninstall -->
        <exclude domain="sharedpref" path="."/>
    </cloud-backup>
    <device-transfer>
        <!-- Exclude SharedPreferences from device transfer -->
        <exclude domain="sharedpref" path="."/>
    </device-transfer>
</data-extraction-rules>